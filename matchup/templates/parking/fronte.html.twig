{% extends 'base2.html.twig' %}

{% block title %}liste des Parkings {% endblock %}

 {% block section1 %}
     <div id="wrapper">


         <!--header end-->

         <!--product-info-->
         <div class="free-shipping-a cart ptb-90">
             <div class="container-fluid">
                 <div class="row justify-contect-center">


                     <div class="col-lg-4 col-md-6 col-sm-6">

                     </div>
                 </div>
             </div>
         </div>
         <!---free shipping-a-->
         <section class="cart-block wfp">
             <div class="container">
                 <div class="row">
                     <div class="col-lg-12">
                         <div class="table-responsive wfp">
                             <table class="table table-bordered table-hover">
                                 <thead>
                                 <tr>
                                     <th class="t-product">IdParking</th>
                                     <th class="t-quantity">Adresse </th>
                                     <th class="t-total">Nbdeplace</th>

                                 </tr>
                                 </thead>
                                 <tbody>

                                 {% for parking in parking %}
                                     <tr>

                                         <td class="t-product">

                                             <div class="cart-product-detail">


                                                 <div class="overlay"></div>


                                                 <div class="cart-product-info">

                                                     {{ parking.idParking }}

                                                 </div>
                                             </div>
                                         </td>

                                         <td class="t-quantity">


                                             {{ parking.adresse }}
                                         </td>


                                         <td class="t-product">

                                             {{ parking.nbdeplace }}
                                         </td>




                                     </tr>



                                     </tr>


                                 {% endfor %}
                                 </tbody>
                             </table>
                         </div>
                     </div>
                 </div>
             </div>
         </section>
     </div>
 {% endblock %}