{% extends 'base.html.twig' %}

{% block title %}Reservationparking index{% endblock %}

{% block body %}
    <h1>Reservationparking index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Idrsvparking</th>
                <th>DateD</th>
                <th>DateF</th>
                <th>Email</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for reservationparking in reservationparkings %}
            <tr>
                <td>{{ reservationparking.idrsvparking }}</td>
                <td>{{ reservationparking.dateD ? reservationparking.dateD|date('Y-m-d') : '' }}</td>
                <td>{{ reservationparking.dateF ? reservationparking.dateF|date('Y-m-d') : '' }}</td>
                <td>{{ reservationparking.email }}</td>
                <td>
                    <a href="{{ path('reservationparking_show', {'idrsvparking': reservationparking.idrsvparking}) }}">show</a>
                    <a href="{{ path('reservationparking_edit', {'idrsvparking': reservationparking.idrsvparking}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('reservationparking_new') }}">Create new</a>
{% endblock %}
